{
    "name": "the-3labs-team/nova-busy-resource-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "The3labsTeam\\NovaBusyResourceField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "The3labsTeam\\NovaBusyResourceField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.16"
    }
}
